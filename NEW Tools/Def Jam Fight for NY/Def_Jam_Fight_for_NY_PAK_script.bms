idstring "PAKN"
get NUMBER_OF_FILES long
get ENTIRES_ARRAY_SIZE long

xmath FILE_OFFSET "12 + ENTIRES_ARRAY_SIZE + 24"


for i = 0 < NUMBER_OF_FILES
	GetDString FILE_NAME 60
	get FILE_SIZE long
	log FILE_NAME FILE_OFFSET FILE_SIZE
	xmath FILE_OFFSET "FILE_OFFSET + FILE_SIZE"
next i
